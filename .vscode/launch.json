  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome",
      "url": "https://scratch.mit.edu",
      "runtimeArgs": ["--load-extension=${workspaceFolder}"]
    },
    {
      "type": "firefox",
      "request": "launch",
      "name": "Launch Firefox",
      "url": "https://scratch.mit.edu",
      "webRoot": "https://scratch.mit.edu",
      "addonPath": "${workspaceFolder}"
    }
  ]
}
